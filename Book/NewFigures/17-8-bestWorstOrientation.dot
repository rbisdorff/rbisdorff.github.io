n4-> n7 [dir=forward, color=black] ;
n5-> n6 [dir=back, color=grey, arrowtail=empty] ;
n5-> n7 [dir=forward, color=black] ;
n6-> n7 [dir=forward, color=black] ;
{rank=max; n2 n4 }
{rank=min; n7 }
}
